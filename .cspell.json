{
  "ignorePaths": [
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/**",
    "**/.pnpm-lock.json",
    ".github/workflows/**",
    ".vscode",
    "megalinter",
    "megalinter-reports/**",
    "package-lock.json",
    "report",
    "renovate-log.txt"
  ],
  "language": "en",
  "noConfigSearch": true,
  "dictionaryDefinitions": [
    {
      "name": "project",
      "path": ".config/cspell/project.txt",
      "addWords": true
    }
  ],
  "dictionaries": ["project"],
  "version": "0.2"
}
